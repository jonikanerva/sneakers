{
  "name": "sneakers",
  "version": "1.0.0",
  "description": "sneakers!",
  "main": "build/app/server.js",
  "repository": "github.com",
  "author": "Jhonny <jhonny@jhonny.org>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^10.16.0",
    "yarn": "^1.16.0"
  },
  "scripts": {
    "start": "node build/server.js",
    "s": "run-p touch watch:build watch:compile",
    "build": "rm -rf build && webpack --mode production",
    "test": "jest",
    "touch": "mkdir build 2>/dev/null; touch build/server.js",
    "lint": "yarn lint:tslint && yarn lint:prettier",
    "lint:prettier": "prettier --list-different --parser typescript --config package.json '**/*.ts'",
    "lint:tslint": "tslint --project .",
    "watch:test": "jest --watchAll --coverage=false",
    "watch:build": "nodemon --watch build --exec 'yarn start'",
    "watch:compile": "webpack --mode development --watch",
    "worker": "node build/fetchAndStore.js",
    "docker:build": "docker build -t registry.gitlab.com/jonikanerva/dok/sneakers:latest .",
    "docker:push": "docker push registry.gitlab.com/jonikanerva/dok/sneakers:latest",
    "deploy": "run-s docker:build docker:push"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jsdom": "^15.1.1",
    "luxon": "^1.17.1",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-on-screen": "^2.1.1",
    "react-router-dom": "^5.0.1",
    "redis": "^2.8.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.15",
    "@types/jsdom": "^12.2.4",
    "@types/luxon": "^1.15.2",
    "@types/node": "^12.6.2",
    "@types/ramda": "^0.26.15",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.4",
    "@types/redis": "^2.8.13",
    "@types/supertest": "^2.0.8",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.0.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.4",
    "webpack-node-externals": "^1.7.2"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  }
}
