{
  "name": "sneakers",
  "version": "1.0.0",
  "description": "sneakers!",
  "main": "build/app/server.js",
  "repository": "github.com",
  "author": "Jhonny <jhonny@jhonny.org>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^10.15.1",
    "yarn": "^1.13.0"
  },
  "scripts": {
    "start": "node build/app/server.js",
    "s": "run-p watch:build watch:compile",
    "build": "tsc",
    "test": "jest",
    "lint": "yarn lint:tslint && yarn lint:prettier",
    "lint:prettier": "prettier --list-different --parser typescript --config package.json '**/*.ts'",
    "lint:tslint": "tslint --project .",
    "heroku-postbuild": "yarn build",
    "watch:test": "jest --watchAll --coverage=false",
    "watch:build": "nodemon --watch build --exec 'yarn start'",
    "watch:compile": "tsc --watch",
    "worker": "node build/app/workers/fetchAndStore.js"
  },
  "dependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/jsdom": "^12.2.2",
    "@types/luxon": "^1.11.0",
    "@types/node": "^11.9.4",
    "@types/ramda": "^0.25.50",
    "@types/redis": "^2.8.10",
    "axios": "^0.18.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "jsdom": "^13.2.0",
    "luxon": "^1.11.2",
    "md5-hex": "^2.0.0",
    "ramda": "^0.26.1",
    "redis": "^2.8.0",
    "typescript": "^3.4.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.5",
    "@types/supertest": "^2.0.7",
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "supertest": "^3.4.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  }
}
