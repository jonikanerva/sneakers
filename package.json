{
  "name": "sneakers",
  "version": "1.0.0",
  "description": "sneakers!",
  "main": "build/app/server.js",
  "repository": "github.com",
  "author": "Jhonny <jhonny@jhonny.org>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^10.15.1",
    "yarn": "^1.13.0"
  },
  "scripts": {
    "start": "node build/app/server.js",
    "s": "run-p watch:build watch:compile",
    "build": "tsc",
    "test": "jest",
    "lint": "yarn lint:tslint && yarn lint:prettier",
    "lint:prettier": "prettier --list-different --parser typescript --config package.json '**/*.ts'",
    "lint:tslint": "tslint --project .",
    "postinstall": "yarn build",
    "watch:test": "jest --watchAll --coverage=false",
    "watch:build": "nodemon --watch build --exec 'yarn start'",
    "watch:compile": "tsc --watch"
  },
  "dependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/jsdom": "^12.2.1",
    "@types/node": "^10.12.21",
    "@types/ramda": "^0.25.47",
    "@types/redis": "^2.8.10",
    "axios": "^0.18.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "jsdom": "^13.2.0",
    "md5-hex": "^2.0.0",
    "ramda": "^0.26.1",
    "redis": "^2.8.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/supertest": "^2.0.7",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.3",
    "supertest": "^3.4.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.3.1"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  }
}
